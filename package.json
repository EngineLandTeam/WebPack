{
  "name": "t3am-webpack",
  "version": "1.0.0",
  "description": "Learn EngineLand in WebPack",
  "main": "./src/main.js",
  "scripts": {
    "build": "webpack --entry ./src/js/index.js --output ./dist/assets/js/main.js --mode development",
    "build:prod": "webpack --mode production",
    "build:external": "webpack --config ./src/dll/webpack.external.js",
    "build:multi": "webpack --config ./src/dll/webpack.multi.js",
    "build:css": "webpack --config ./src/dll/webpack.css.js",
    "build:plugins": "webpack --config ./src/dll/webpack.plugins.js",
    "build:extract:css": "webpack --config ./src/dll/webpack.extract-css.js",
    "build:dev": "webpack-dev-server --config ./src/dll/webpack.dev-server.js",
    "build:babel": "webpack --config ./src/dll/webpack.babel.js",
    "build:fonts": "webpack --config ./src/dll/webpack.fonts.js",
    "build:images": "webpack --config ./src/dll/webpack.images.js",
    "build:video": "webpack --config ./src/dll/webpack.video.js",
    "build:json": "webpack --config ./src/dll/webpack.json.js",
    "build:react": "webpack --config ./src/dll/webpack.react.js",
    "build:vue": "webpack --config ./src/dll/webpack.vue.js",
    "build:sass": "webpack --config ./src/dll/webpack.sass.js",
    "build:stylus": "webpack --config ./src/dll/webpack.stylus.js",
    "build:less": "webpack --config ./src/dll/webpack.less.js",
    "build:postcss": "webpack --config ./src/dll/webpack.postcss.js",
    "build:prevent": "webpack --config ./src/dll/webpack.prevent.js",
    "build:vendor": "webpack --config ./src/dll/webpack.vendor.js",
    "build:dll": "webpack --config ./src/dll/webpack.dll.js",
    "build:dll:src": "webpack --config ./src/dll/webpack.dll-src.js",
    "build:dynamic": "webpack --config ./src/dll/webpack.dynamic.js"
  },
  "keywords": [
    "Svelte",
    "Webpack",
    "Bootstrap",
    "Babel",
    "FontAwesome",
    "Sass",
    "Css"
  ],
  "author": "@T3am. <EngineLandTeam@gmail.com> (https://EngineLand.dev)",
  "license": "MIT",
  "devDependencies": {
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
